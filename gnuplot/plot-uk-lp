set output "../graphs/covid-uk-lp.png"
set logscale y
offset = 55
set key at -1,50 box
set yrange [5:4000]
set xrange [5 - 55:]
set ylabel "COVID19 cases per million inhabitants"
set xlabel "Days (from 10 April 2020)"
set mxtics
set grid mxtics xtics ytics mytics
