set grid
plot "../aligned/country_data/italy" using ($1):($2/itpop) w lp lc "#a0a0a0" lw 2 t "All of Italy", \
"../aligned/uk/uk-regions-cum" using ($1 - lonoff):($3/lonpop) w lp lw 2 lc "red" t sprintf("London, UK (%d days behind Italy)", lonoff), \
"../aligned/uk/uk-regions-cum" using ($1 - midoff):($4/midpop) w lp lw 2 lc "#00a000" t sprintf("Midlands, UK (%d days behind Italy)", midoff), \
"../aligned/uk/uk-regions-cum" using ($1 - seoff):($7/sepop) w lp lw 2 lc "#00a0ff" t sprintf("South East, UK (%d days behind Italy)", seoff), \
"../aligned/uk/uk-regions-cum" using ($1 - neoff):($5/nepop) w lp lw 2 lc "blue" t sprintf("North East, UK (%d days behind Italy)", neoff), \
"../aligned/uk/uk-regions-cum" using ($1 - nwoff):($6/nwpop) w lp lw 2 lc "brown" t sprintf("North West (%d days behind Italy)", nwoff), \
"../aligned/uk/uk-regions-cum" using ($1 - eoff):($2/epop) w lp lw 2 lc "orange" t sprintf("UK, East (%d days behind Italy)", eoff), \
"../aligned/uk/uk-regions-cum" using ($1 - swoff):($8/swpop) w lp lw 2 lc "magenta" t sprintf("South West, UK, (%d days behind Italy)", swoff),\
1.35**(x+offset - 4) lc "black"  dt 4 t "35% daily increase",\
1.22**(x+offset + 2) lc "black" dashtype 2 t "22% daily increase", \
1.16**(x+offset + 9) lc "black" dashtype 3 t "16% daily increase"

